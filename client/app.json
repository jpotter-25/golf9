{
  "expo": {
    "name": "Golf 9",
    "slug": "golf9",
    "scheme": "golf9",
    "version": "0.1.0",
    "orientation": "portrait",
    "newArchEnabled": true,
    "sdkVersion": "53.0.0",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0B1023"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0B1023"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": []
  }
}